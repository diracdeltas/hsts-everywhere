<ruleset name="Minus.com (partial)" platform="mixedcontent">

	<target host="min.us" />
	<target host="*.min.us" />
	<target host="minus.com" />
	<target host="*.minus.com" />
		<exclusion pattern="^http://(blog|feedback)\." />


	<!--	Members have unique subdomains.
		Cert only matches (*.)minus.com.	-->
	<rule from="^https?://([^/:@\.]+\.)?min(?:\.us|us\.com)/"
		to="https://$1minus.com/" />

</ruleset>

