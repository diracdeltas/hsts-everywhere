<ruleset name="OpenX (partial)">
	<target host="i-cdn.openx.com" />
	<target host="d1.openx.org" />
	<target host="developer.openx.org" />

	<securecookie host="^i-cdn\.openx\.com$" name=".+" />
	<securecookie host="^(d1|developer)\.openx\.org$" name=".+" />

	<rule from="^http://i-cdn\.openx\.com/" to="https://i-cdn.openx.com/" />
	<rule from="^http://(d1|developer)\.openx\.org/" to="https://$1.openx.org/" />
</ruleset>