<!--
	Other US government rulesets:

		- US-government.xml
		- US-government-mismatches.xml

-->
<ruleset name="USA.gov (partial)">

	<target host="usa.gov" />
	<target host="*.usa.gov" />


	<!--	Observed cookie domains:

			- publications
			- search

				-->
	<securecookie host="^.*\.usa\.gov$" name=".*" />


	<rule from="^http://(business|publications|search)\.usa\.gov/"
		to="https://$1.usa.gov/" />

</ruleset>
