<ruleset name="Interactive Advertising Bureau" platform="mixedcontent">

	<target host="iab.net" />
	<target host="www.iab.net" />


	<securecookie host="^www\.iab\.net$" name=".*" />


	<!--	Cert doesn't match !www.	-->
	<rule from="^http://(?:www\.)?iab\.net/"
		to="https://www.iab.net/" />

</ruleset>
