<ruleset name="Mandriva.com">
	<target host="mandriva.com" />
	<target host="www.mandriva.com" />
	<target host="webapps.mandriva.com" />
	<rule from="^http://mandriva\.com/" to="https://mandriva.com/"/>
	<rule from="^http://www\.mandriva\.com/" to="https://www.mandriva.com/"/>
	<rule from="^http://webapps\.mandriva\.com/" to="https://webapps.mandriva.com/"/>
</ruleset>

