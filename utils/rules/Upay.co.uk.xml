<ruleset name="Upay" platform="mixedcontent">
  <target host="upay.co.uk" />
  <target host="www.upay.co.uk" />
  <target host="upaymobile.co.uk" />
  <target host="www.upaymobile.co.uk" />

  <rule from="^http://(?:www\.)?upay\.co\.uk/" to="https://www.upay.co.uk/" />
  <rule from="^http://(?:www\.)?upaymobile\.co\.uk/" to="https://www.upaymobile.co.uk/" />
</ruleset>
