<ruleset name="ON24 (partial)" platform="mixedcontent">

	<target host="on24.com"/>
	<target host="*.on24.com"/>

	<securecookie host="^(event\.|www\.)?on24\.com$" name=".*"/>

	<rule from="^http://(?:www\.)?on24\.com/"
		to="https://www.on24.com/"/>

	<rule from="^http://(event|w)\.on24\.com/"
		to="https://$1.on24.com/"/>

</ruleset>
