<ruleset name="WinRAR">

	<target host="win-rar.com" />
	<!--	* for cross-domain cookies.	-->
	<target host="*.win-rar.com" />


	<securecookie host="^.*\.win-rar\.com$" name=".*" />


	<!--	Cert only matches www.	-->
	<rule from="^http://(?:www\.)?win-rar\.com/"
		to="https://www.win-rar.com/" />

</ruleset>
