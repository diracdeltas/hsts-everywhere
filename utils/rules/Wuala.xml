<ruleset name="wuala">
  <target host="wuala.com" />
  <target host="*.wuala.com" />

  <securecookie host="^(?:(www|forum|stats|thumb\d+)\.)?wuala\.com$" name=".*"/>

  <rule from="^http://wuala\.com/" to="https://wuala.com/"/>
  <rule from="^http://(www|forum|stats|thumb\d+)\.wuala\.com/" to="https://$1.wuala.com/"/>
</ruleset>
