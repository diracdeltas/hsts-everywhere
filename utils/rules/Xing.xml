<ruleset name="Xing">
  <target host="xing.com" />
  <target host="*.xing.com" />

  <rule from="^http://(?:www\.)?xing\.com/" to="https://www.xing.com/"/>

  <rule from="^http://corporate\.xing\.com/" to="https://corporate.xing.com/"/>

  <securecookie host="^(?:corporate|www)\.xing\.com$" name=".*"/>
</ruleset>
