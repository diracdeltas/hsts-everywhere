<ruleset name="etracker">

	<target host="etracker.com"/>
	<target host="*.etracker.com"/>

	<securecookie host="^(.*\.)?etracker\.com$" name=".*"/>

	<rule from="^http://etracker\.com/"
		to="https://www.etracker.com/"/>

	<rule from="^http://(\w+)\.etracker\.com/"
		to="https://$1.etracker.com/"/>

</ruleset>
