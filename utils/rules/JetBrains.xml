<!--
	Nonfunctional subdomains:

		- blogs		(refused)

-->
<ruleset name="JetBrains (partial)" platform="mixedcontent">

	<target host="jetbrains.com"/>
	<target host="*.jetbrains.com"/>

	<securecookie host="^(.*\.)?jetbrains\.com$" name=".*"/>

	<rule from="^http://(?:www\.)?jetbrains\.com/"
		to="https://www.jetbrains.com/"/>

</ruleset>
