<ruleset name="Appache (partial)">

	<target host="5apps.com"/>
	<target host="develop.5apps.com"/>
	<target host="www.5apps.com"/>

	<securecookie host="^5apps\.com$" name=".*"/>

	<rule from="^http://(?:www\.)?5apps\.com/"
		to="https://5apps.com/"/>

	<rule from="^http://develop\.5apps\.com/"
		to="https://develop.5apps.com/"/>

</ruleset>
