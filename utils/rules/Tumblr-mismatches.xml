<!--
	For rules that are on by default, see Tumblr.xml.

-->
<ruleset name="Tumblr (mismatches/broken)" default_off="mismatch">

	<!--	Akamai.
				-->
	<target host="a.tumblr.com" />


	<rule from="^http://a\.tumblr\.com/"
		to="https://a.tumblr.com/" />

	<rule from="^https?://(?:secure\.)?assets\.tumblr\.com/"
		to="https://secure.assets.tumblr.com/" />


</ruleset>
