<ruleset name="eGenix.com" default_off="self-signed">

	<target host="egenix.com"/>
	<target host="*.egenix.com"/>


	<!--	Cert doesn't match !www.	-->
	<rule from="^http://(?:www\.)?egenix\.com/"
		to="https://www.egenix.com/"/>

	<rule from="^http://downloads\.egenix\.com/"
		to="https://downloads.egenix.com/" />

</ruleset>
