<!--
	for Zendesk clients which otherwise have no rules.

-->
<ruleset name="Zendesk.com clients (partial)">

	<target host="support.photobucket.com" />
	<target host="support.voxer.com" />


	<rule from="^http://support\.(photobucket|voxer)\.com/(assets|generated|images|registration|system)/"
		to="https://$1.zendesk.com/$2/" />

</ruleset>
