	Nonfunctional:

	-  h10060.www1	(cert doesn't match; 404)

-->
<ruleset name="Hewlett-Packard Company (buggy)" default_off="https://eff.org/r.4ap5">

	<target host="compaq.com"/>
	<target host="www.compaq.com"/>
	<target host="eds.com"/>
	<target host="*.eds.com"/>
	<target host="hp.com"/>
	<target host="esca2.americas.hp.com"/>
	<target host="esca5.asiapac.hp.com"/>
	<target host="*.austin.hp.com"/>
	<target host="esca3.europe.hp.com"/>
	<target host="*.hp.com"/>
	<target host="www.*.hp.com"/>
	<target host="*.www1.hp.com"/>
		<!--	404s over http	-->
		<exclusion pattern="^http://h18000\.www1\.hp\.com/products/servers/linux/documentation\.html" />
	<target host="*.www2.hp.com"/>
	<target host="h41183.www4.hp.com"/>
	<target host="www8.hp.com"/>
	<target host="*.www8.hp.com"/>
	<target host="*.hp-ww.com"/>
	<target host="by.optimost.com"/>
	<target host="by.essl.optimost.com"/>
	<target host="threatlinq.tippingpoint.com"/>
	<target host="www.threatlinq.tippingpoint.com"/>


	<securecookie host="^esca2\.americas\.hp\.com$" name=".*"/>
	<securecookie host="^esca(5\.asiapac|3\.europe)\.hp\.com$" name=".*"/>
	<securecookie host="^((www\.)?register|shopping)\.hp\.com$" name=".*"/>
	<securecookie host="^h(20141\.www2|41183\.www4)\.hp\.com$" name=".*"/>
	<!--	Stats cookie set by met1.hp.com		-->
	<securecookie host="^\.hp\.com$" name="^s_vi$"/>


	<rule from="^https?://3com\.com/"
		to="https://www.3com.com/"/>

	<rule from="^http://(b2bgw|login|www)\.3com\.com/"
		to="https://$1.3com.com/"/>

	<rule from="^http://(?:www\.)?compaq\.com/"
		to="https://www.compaq.com/"/>

	<rule from="^http://(www(-mo)?\.)?eds\.com/"
		to="https://$1eds.com/"/>

	<rule from="^https?://hp\.com/"
		to="https://www.hp.com/"/>

	<rule from="^http://www\.hp\.com/img/"
		to="https://www.hp.com/img/"/>

	<rule from="^http://ftp\.hp\.com/"
		to="https://ftp.hp.com/"/>

	<rule from="^http://(www\.)?register\.hp\.com/"
		to="https://$1register.hp.com/"/>

	<rule from="^http://(www\.)?shopping\.hp\.com/"
		to="https://shopping.hp.com/"/>

	<rule from="^http://welcome\.hp\.com/"
		to="https://secure.hp-ww.com/"/>

	<rule from="^http://h(1000[34]|10018|10032|10048|10057|1008[456]|10148|10151|10176|1700[3-9]|17010|18000)\.www1\.hp\.com/"
		to="https://h$1.www1.hp.com/"/>

	<rule from="^http://h20141\.www2\.hp\.com/"
		to="https://h20141.www2.hp.com/"/>

	<rule from="^http://esca2\.americas\.hp\.com/"
		to="https://esca2.americas.hp.com/"/>

	<rule from="^http://esca5\.asiapac\.hp\.com/"
		to="https://esca5.asiapac.hp.com/"/>

	<rule from="^http://esam(2)?\.austin\.hp\.com/"
		to="https://esam$1.austin.hp.com/"/>

	<rule from="^http://(?:vausnzpro2\.austin|20465\.www2)\.hp\.com/"
		to="https://vausnzpro2.austin.hp.com/"/>

	<rule from="^http://(?:www\.)?esca\.hp\.com/"
		to="https://esca2.americas.hp.com/"/>

	<rule from="^http://esca3\.europe\.hp\.com/"
		to="https://esca3.europe.hp.com/"/>

	<rule from="^http://h41183\.www4\.hp\.com/"
		to="https://h41183.www4.hp.com/"/>

	<rule from="^http://(?:ssl|www\.)?www8\.hp\.com/"
		to="https://ssl.www8.hp.com/"/>

	<rule from="^http://(?:secure|welcome)\.hp-ww\.com/"
		to="https://secure.hp-ww.com/"/>

	<rule from="^http://by(?:\.essl)?\.optimost\.com/"
		to="https://by.essl.optimost.com/"/>

	<rule from="^http://(www\.)?threatlinq\.tippingpoint\.com/"
		to="https://$1threatlinq.tippingpoint.com/"/>

</ruleset>
