<!--	Nonfunctional:
		(media.|topliners.|www.)eloqua.com
		eloquaexperience.com
		*.en25.com

	Problematic domains in Eloqua-mismatches.xml
-->
<ruleset name="Eloqua">

	<target host="elq.to"/>
	<target host="www.elq.to"/>
	<target host="secure.eloqua.com"/>

	<rule from="^http://(?:www\.)?elq\.to/"
		to="https://bit.ly/"/>

	<rule from="^http://secure\.eloqua\.com/"
		to="https://secure.eloqua.com/"/>

</ruleset>
