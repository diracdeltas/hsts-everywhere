<!--
	Other Gajim rulesets:

		- Gajim-CAcert.xml


	Nonfunctional:

		- hq	(cert: trac.gajim.org; redirects to http)

-->
<ruleset name="Gajim (partial)">

	<target host="gajim.org" />
	<target host="*.gajim.org" />


	<securecookie host="^(.*\.)?gajim\.org$" name=".*" />


	<rule from="^http://(www\.)?gajim\.org/"
		to="https://$1gajim.org/" />

</ruleset>
