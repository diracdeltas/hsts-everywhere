<ruleset name="Velaro (partial)">

	<target host="c.velaro.com"/>
	<target host="service.velaro.com"/>
	<target host="sp.velaro.com"/>

	<securecookie host="^.*\.velaro.com$" name=".*"/>

	<rule from="^http://(c|service|sp)\.velaro\.com/"
		to="https://$1.velaro.com/"/>

</ruleset>
