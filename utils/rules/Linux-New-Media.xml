<ruleset name="Linux New Media (partial)">

	<target host="*.linuxnewmedia.de" />
	<target host="*.shop.linuxnewmedia.de" />


	<securecookie host="^.*\.linuxnewmedia\.de$" name=".*" />


	<rule from="^http://shop\.linuxnewmedia\.de/"
		to="https://shop.linuxnewmedia.de/" />

</ruleset>
