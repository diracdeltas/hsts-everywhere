<ruleset name="ChillingEffects">

	<target host="chillingeffects.org" />
	<target host="*.chillingeffects.org" />

	<!--	Cert doesn't match !www.	-->
	<rule from="^http://(?:www\.)?chillingeffects\.org/"
		to="https://www.chillingeffects.org/" />

	<rule from="^http://images\.chillingeffects\.org/"
		to="https://images.chillingeffects.org/" />

</ruleset>
