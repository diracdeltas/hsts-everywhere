<ruleset name="CoinDL">

	<target host="coindl.com" />
	<target host="www.coindl.com" />
	<!--	* for cross-domain cookie.	-->
	<target host="*.www.coindl.com" />

	<securecookie host="^\.?www\.coindl\.com$" name=".*" />

	<rule from="^http://(www\.)?coindl\.com/"
		to="https://$1coindl.com/" />

</ruleset>
