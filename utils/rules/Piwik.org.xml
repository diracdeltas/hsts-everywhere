<ruleset name="piwik.org (partial)" default_off="expired, self-signed">

	<target host="piwik.org" />
	<target host="de.piwik.org" />
	<target host="el.piwik.org" />
	<target host="fr.piwik.org" />
	<target host="pl.piwik.org" />
	<target host="www.piwik.org" />

	<rule from="^http://(www\.)?([d-p]\w\.)?piwik\.org/"
		to="https://$2piwik.org/" />

</ruleset>
