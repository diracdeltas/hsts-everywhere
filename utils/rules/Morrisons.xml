<ruleset name="Morrisons">

	<target host="morrisons.co.uk" />
	<!--	* for cross-domain cookie.	-->
	<target host="*.morrisons.co.uk" />


	<securecookie host="^(.*\.)?morrisons\.co\.uk$" name=".*" />


	<rule from="^http://(www\.)?morrisons\.co\.uk/"
		to="https://$1morrisons.co.uk/" />

</ruleset>
