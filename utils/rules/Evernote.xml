<ruleset name="Evernote">

	<target host="evernote.com" />
	<target host="www.evernote.com" />


	<securecookie host="^(.*\.)?evernote\.com$" name=".*" />


	<rule from="^http://(www\.)?evernote\.com/"
		to="https://$1evernote.com/" />

</ruleset>
