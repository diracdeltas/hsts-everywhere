<ruleset name="Surveydaddy" platform="mixedcontent">
  <target host="surveydaddy.com" />
  <target host="*.surveydaddy.com" />

  <exclusion pattern="^http://support\.surveydaddy\.com/"/>

  <rule from="^http://surveydaddy\.com/"
	to="https://surveydaddy.com/" />
  <rule from="^http://([^@:\./]+)\.surveydaddy\.com/"
	to="https://$1.surveydaddy.com/" />
</ruleset>
