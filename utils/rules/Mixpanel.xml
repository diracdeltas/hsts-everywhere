<ruleset name="Mixpanel (partial)">

	<target host="mixpanel.com"/>
	<target host="*.mixpanel.com"/>
		<exclusion pattern="^http://blog\."/>

	<securecookie host="^(.*\.)?mixpanel\.com$" name=".*"/>

	<rule from="^http://(\w+\.)?mixpanel\.com/"
		to="https://$1mixpanel.com/"/>

</ruleset>
