<ruleset name="Newzbin">
  <target host="newzbin.com" />
  <target host="newzbin2.es" />
  <target host="*.newzbin.com" />
  <target host="*.newzbin2.es" />

  <rule from="^http://(?:www\.)?newzbin(?:\.com|2\.es)/?" to="https://www.newzbin2.es/"/>
  <rule from="^http://docs\.newzbin2\.es/?" to="https://docs.newzbin2.es/"/>
</ruleset>
