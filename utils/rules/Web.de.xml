<ruleset name="web.de (buggy)" default_off="breaks videos">
  <target host="web.de"/>
  <target host="*.web.de"/>
  <target host="www.web.de"/>
  <target host="logout.webde.uimserv.net"/>

  <securecookie host="^(.*\.)?web\.de$" name=".*" />

  <rule from="^http://(?:www\.)?web\.de/" to="https://web.de/"/>
  <rule from="^http://web\.de/" to="https://web.de/"/>
  <rule from="^http://logout\.webde\.uimserv\.net/" to="https://logout.webde.uimserv.net/"/>
</ruleset>
