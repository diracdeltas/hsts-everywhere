<ruleset name="Demonoid">
  <target host="*.demonoid.com" />
  <target host="demonoid.com" />
  <target host="*.demonoid.me" />
  <target host="demonoid.me" />
  <target host="*.demonoid.ph" />
  <target host="demonoid.ph" />

  <securecookie host="^(.*\.)demonoid\.com$" name=".*" />
  <securecookie host="^(.*\.)demonoid\.me$" name=".*" />
  <securecookie host="^(.*\.)demonoid\.ph$" name=".*" />

  <rule from="^http://(?:www\.)?demonoid\.com/" to="https://www.demonoid.com/"/>
  <rule from="^http://(?:www\.)?demonoid\.me/" to="https://www.demonoid.me/"/>
  <rule from="^http://(?:www\.)?demonoid\.ph/" to="https://www.demonoid.ph/"/>
</ruleset>
