<ruleset name="NetBeans (partial)">

	<target host="forums.netbeans.org"/>


	<securecookie host="^forums\.netbeans\.org$" name=".*"/>


	<rule from="^http://forums\.netbeans\.org/"
		to="https://forums.netbeans.org/"/>

</ruleset>
