<ruleset name="dianomi (partial)" platform="mixedcontent">

	<target host="dianomi.com"/>
	<!-- /cdn. hosted on CloudFront	-->
	<target host="*.dianomi.com"/>

	<securecookie host="^(.*\.)?dianomi\.com$" name=".*"/>

	<rule from="^http://(?:cdn\.|www\.)?dianomi\.com/"
		to="https://www.dianomi.com/"/>

</ruleset>
