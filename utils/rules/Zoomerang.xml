<ruleset name="Zoomerang (partial)" platform="mixedcontent">

	<target host="zoomerang.com" />
	<target host="*.zoomerang.com" />


	<rule from="^http://(?:www\.)?zoomerang\.com/Survey/((?:App_)?Themes/|TinyMCE\.ashx|ZoomStatic/)"
		to="https://www.zoomerang.com/Survey/$1" />

	<rule from="^http://static\.zoomerang\.com/"
		to="https://static.zoomerang.com/" />

</ruleset>
