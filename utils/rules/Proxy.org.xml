<ruleset name="Proxy.org (partial)" platform="mixedcontent">

	<target host="proxy.org"/>
		<exclusion pattern="^http://proxy\.org/forum/"/>
	<target host="*.proxy.org"/>

	<!--	encountered:
			- proxy.org
			- .proxy.org		-->
	<securecookie host="^(.*\.)?proxy\.org$" name=".*"/>

	<rule from="^http://(www\.)?proxy\.org/"
		to="https://$1proxy.org/"/>

</ruleset>
