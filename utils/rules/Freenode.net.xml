<ruleset name="Freenode.net (CAcert)" platform="cacert">
  <target host="freenode.net" />
  <target host="*.freenode.net" />

  <rule from="^http://(?:www\.)?freenode\.net/" to="https://www.freenode.net/"/>
  <rule from="^http://(blog|dev|webchat)\.freenode\.net/"
          to="https://$1.freenode.net/"/>
</ruleset>
