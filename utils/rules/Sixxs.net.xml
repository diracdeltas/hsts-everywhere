<!--
	Problematic domains:

		- sixxs.net	(mismatched)

-->
<ruleset name="Sixxs.net" platform="cacert mixedcontent">

	<target host="sixxs.net" />
	<target host="*.sixxs.net" />


	<securecookie host="^\.sixxs\.net$" name=".+" />


	<rule from="^http://(?:www\.)?sixxs\.net/"
		to="https://www.sixxs.net/" />

</ruleset>
