<!--
	For problematic rules, see Asana-mismatches.xml.


	asana_static.s3.amazonaws.com

-->
<ruleset name="Asana (partial)">

	<target host="asana.com" />
	<target host="*.asana.com" />


	<securecookie host="^app\.asana\.com$" name=".*" />


	<rule from="^https?://(?:www\.)?asana\.com/favicon\.ico"
		to="https://app.asana.com/favicon.ico" />

	<rule from="^http://app\.asana\.com/"
		to="https://app.asana.com/" />

</ruleset>
