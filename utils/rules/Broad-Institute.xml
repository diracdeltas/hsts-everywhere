<ruleset name="Broad Institute">

	<target host="broadinstitute.org" />
	<!--	* for cross-domain cookie.	-->
	<target host="*.broadinstitute.org" />


	<securecookie host="\.broadinstitute\.org$" name=".*" />


	<rule from="^https?://(?:www\.)?broadinstitute\.org/"
		to="https://www.broadinstitute.org/" />

</ruleset>
