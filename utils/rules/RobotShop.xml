<ruleset name="RobotShop (partial)">

	<target host="robotshop.com" />
	<target host="*.robotshop.com" />


	<securecookie host="^(.*\.)?robotshop\.com$" name=".*" />


	<!--	Cert is only valid for secure.	-->
	<rule from="^https?://(secure\.|www\.)?robotshop\.com/"
		to="https://secure.robotshop.com/" />

</ruleset>
